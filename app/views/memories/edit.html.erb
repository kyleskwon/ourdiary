<% @page_title = " | OurMemories" %>

  <% content_for :google_maps do %>
    <script>
      $(document).ready(function() {
        console.log("loaded!");
        <% @memory_markers.each do |marker| %>
          setMarker(<%= marker[:lat] %>, <%= marker[:long] %>, "<%= marker[:title] %>");
        <% end %>
      });
    </script>
  <% end %>

<div class="container">
  <h1 class="center">Edit Memory</h1>
    <%= render 'form' %>
</div>
